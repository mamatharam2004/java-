import java.util.Scanner;
class HelloWorld {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=1;
        int b=sc.nextInt();
        while(a<=b) {
            System.out.println(a);
            a++;
        }
        for(int i=1;i<=b;i++) {
            System.out.println(i);
        }
    }
}
